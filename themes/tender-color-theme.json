			"settings": {
				"comment": "Not Written",
				"foreground": "#eeeeee"
			}
		},
		{
			"name": "Entity Types",
			"scope": [
				"support.type"
			],
			"settings": {
				"comment": "Not Written",
				"foreground": "#eeeeee"
			}
		},
		{
			"name": "CSS Class and Support",
			"scope": [
