{
	"name": "Tender",
	"type": "dark",
	"colors": {
		"editor.background": "#282828",
		"editor.foreground": "#eeeeee",
		"activityBarBadge.background": "#73cef4",
		"sideBarTitle.foreground": "#dadada"
	},
	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"comment": "hi Comment",
				"fontStyle": "italic",
				"foreground": "#666666"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"string constant.other.placeholder"
			],
			"settings": {
				"comment": "hi typescriptVariable",
				"foreground": "#b3deef"
			}
		},
		{
			"name": "Colors",
			"scope": [
				"constant.other.color"
			],
			"settings": {
				"comment": "tender.yml text",
				"foreground": "#eeeeee"
			}
		},
		{
			"name": "Invalid",
			"scope": [
				"invalid",
				"invalid.illegal"
			],
			"settings": {
				"comment": "hi Error",
				"foreground": "#f43753"
			}
		},
		{
			"name": "Keyword, Storage",
			"scope": [
				"keyword",
				"storage.type",
				"storage.modifier"
			],
			"settings": {
				"comment": "Not Written. but blue1 is best",
				"foreground": "#b3deef"
			}
		},
		{
			"name": "Keyword Control",
			"scope": [
				"keyword.control",
			],
			"settings": {
				"comment": "keyword example> if, try, end while. green1 is best",
				"foreground": "#c9d05c"
			}
		},
		{
			"name": "Operator, Misc",
			"scope": [
				"constant.other.color",
				"punctuation",
				"meta.tag",
				"punctuation.definition.tag",
				"punctuation.separator.inheritance.php",
				"punctuation.definition.tag.html",
				"punctuation.definition.tag.begin.html",
				"punctuation.definition.tag.end.html",
				"punctuation.section.embedded",
				"keyword.other.template",
				"keyword.other.substitution"
			],
			"settings": {
				"comment": "base.yml Keyword -> Statement -> blue1",
				"foreground": "#b3deef"
			}
		},
		{
			"name": "Tag",
			"scope": [
				"entity.name.tag",
				"meta.tag.sgml",
				"markup.deleted.git_gutter"
			],
			"settings": {
				"comment": "base.yml Tag -> Special -> blue2",
				"foreground": "#73cef4"
			}
		},
		{
			"name": "Function, Special Method",
			"scope": [
				"entity.name.function",
				"meta.function-call",
				"variable.function",
				"support.function",
				"keyword.other.special-method"
			],
			"settings": {
				"comment": "hi Function",
				"foreground": "#b3deef"
			}
		},
		{
			"name": "Block Level Variables",
			"scope": [
				"meta.block variable.other"
			],
			"settings": {
				"comment": "Not Written",
				"foreground": "#eeeeee"
			}
		},
		{
			"name": "Other Variable, String Link",
			"scope": [
				"support.other.variable",
				"string.other.link"
			],
			"settings": {
				"comment": "Not Written",
				"foreground": "#eeeeee"
			}
		},
		{
			"name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
			"scope": [
				"constant.numeric",
				"constant.language",
				"support.constant",
				"constant.character",
				"constant.escape",
				"variable.parameter",
				"keyword.other.unit",
				"keyword.other"
			],
			"settings": {
				"comment": "base.yml Number, Constant -> yellow2",
				"foreground": "#ffc24b"
			}
		},
		{
			"name": "String, Symbols, Inherited Class, Markup Heading",
			"scope": [
				"string",
				"constant.other.symbol",
				"constant.other.key",
				"entity.other.inherited-class",
				"markup.heading",
				"markup.inserted.git_gutter",
				"meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
			],
			"settings": {
				"comment": "hi String",
				"foreground": "#d3b987"
			}
